{
  "name": "zzw_theme_vertical",
  "version": "0.1",
  "authors": [
    "William Scales <william.scales@zzw.ca>"
  ],
  "description": "ZZW vertical theme",
  "license": "Apache License, Version 2.0",
  "homepage": "https://github.com/williamscales/zzw_theme_vertical",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.1"
  },
  "overrides": {
      "bootstrap": {
          "main": [
              "less/!(variables).less",
              "less/!(navs).less",
              "less/!(type).less", 
              "less/!(grid).less", 
              "less/mixins/*",
              "dist/css/bootstrap-theme.min.css",
              "dist/fonts/*",
              "dist/js/bootstrap.min.js"
          ]
      }
  }
}
